/* pages/activity/activity.wxss */
.cont{background: #ffffff;}
.tant{position: relative}
 .contant{position: absolute;width: 300rpx;color: #ffffff;top: 40rpx;left: 50%;margin-left: -150rpx;text-align: center}
.look_title{padding: 20rpx 0;}
.nameImg{margin-top: 20rpx}
.names{line-height: 40rpx} 

 .nameImg .head{width:40rpx;height:40rpx;background:#f1f1f1;border-radius: 50%;margin-right: 10rpx}
.nameImg .btn{margin-left: 20rpx;font-size: 22rpx;background: #1dafaa;padding: 6rpx;border-radius: 6rpx}

.tant_txt{position: absolute;left: 30rpx;bottom: 30rpx;color: #ffffff;line-height: 44rpx;width: 160rpx; border: solid 1rpx #ffffff;text-align: center;border-radius: 40rpx}

.store_info_btnImg{width: 24rpx;height: 24rpx; background: #ffffff;margin-left: 10rpx;margin-right: 10rpx}


.tant_com{position: absolute;right: 30rpx;bottom: 30rpx;color: #ffffff;line-height: 44rpx;width: 160rpx; border: solid 1rpx #orange;text-align: center;border-radius: 40rpx;background:orange}

.pad{padding: 0 30rpx}

.cont_tant{border-bottom: 1rpx solid #eeeeee;padding-bottom: 10rpx}
.cont_title{background: #f1f1f1;line-height: 98rpx}
.cont_box{margin-left: 20rpx}
.cont_day{margin-top: 20rpx}

.contants{margin-top: 20rpx}
._week_cant{display: flex;height: 100rpx;background: #f8f8f8;line-height: 100rpx;text-align: center}
._week_cant ._week{flex: 1}
._subYears_item_f{width: 90rpx;height: 90rpx;margin: 0 10rpx}
._subYears_item{width: 90rpx;height: 90rpx;text-align: center;border-radius: 50%;border:solid 1rpx #999999;line-height: 90rpx;margin:0 10rpx}
.year{width: 750rpx;height: 100rpx;border-bottom:solid 1rpx #c8c8c8;border-top:solid 1rpx #c8c8c8;line-height: 100rpx;font-size: 34rpx;color: #111111}

.cont_now{width: 300rpx;height: 300rpx;border:solid 10rpx #1dafaa;border-radius: 50%;margin: 40rpx auto 30rpx auto}

.cont_txt{width: 300rpx; margin-left: auto;margin-right: auto;text-align: center}